<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="480.0" prefWidth="640.0" spacing="20.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="on22.medienprojekt.ImportController">
   <children>
      <BorderPane maxHeight="720.0" maxWidth="1280.0" minHeight="480.0" minWidth="640.0" prefHeight="480.0" prefWidth="640.0">
         <top>
            <ButtonBar buttonMinWidth="148.0" maxHeight="200.0" maxWidth="640.0" prefHeight="79.0" prefWidth="621.0" BorderPane.alignment="CENTER">
              <buttons>
                <Button id="startButton" fx:id="startButton" alignment="TOP_CENTER" minWidth="148.0" mnemonicParsing="false" onAction="#switchToStart" text="Start">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Button>
                  <Button id="managementButton" fx:id="managementButton" alignment="TOP_CENTER" minWidth="148.0" mnemonicParsing="false" onAction="#switchToManagement" text="Verwaltung">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Button>
                  <Button id="taglistButton" fx:id="taglistButton" alignment="TOP_CENTER" minWidth="148.0" mnemonicParsing="false" onAction="#switchToTaglist" text="Tagliste">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Button>
                  <Button id="importButton" fx:id="importButton" alignment="TOP_CENTER" defaultButton="true" minWidth="148.0" mnemonicParsing="false" onAction="#switchToImport" text="Import">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Button>
              </buttons>
               <BorderPane.margin>
                  <Insets left="-20.0" />
               </BorderPane.margin>
            </ButtonBar>
         </top>
         <center>
            <Label text="Import View" BorderPane.alignment="CENTER">
               <font>
                  <Font size="32.0" />
               </font>
            </Label>
         </center>
      </BorderPane>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
