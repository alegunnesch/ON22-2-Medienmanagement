<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="720.0" prefWidth="1280.0" spacing="20.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="on22.medienprojekt.ManagementController">
   <children>
      <BorderPane prefHeight="485.0" prefWidth="1242.0">
         <left>
            <AnchorPane fx:id="Anchor" prefHeight="430.0" prefWidth="194.0" BorderPane.alignment="CENTER">
               <children>
                  <TreeView fx:id="treeView" layoutY="-4.0" onContextMenuRequested="#selectItem" onMouseClicked="#selectItem" prefHeight="434.0" prefWidth="200.0" />
               </children>
            </AnchorPane>
         </left>
         <top>
          <ButtonBar buttonMinWidth="110.0" prefHeight="62.0" prefWidth="1242.0" BorderPane.alignment="CENTER">
              <buttons>
                <Button fx:id="startButton" alignment="CENTER" mnemonicParsing="false" onAction="#switchToStart" text="Start" translateX="-200.0">
                     <font>
                        <Font size="32.0" />
                     </font>
                  </Button>
                  <Button fx:id="managementButton" alignment="CENTER" defaultButton="true" mnemonicParsing="false" onAction="#switchToManagement" text="Verwaltung" translateX="-200.0">
                     <font>
                        <Font size="32.0" />
                     </font>
                  </Button>
                  <Button fx:id="taglistButton" alignment="TOP_CENTER" mnemonicParsing="false" onAction="#switchToTaglist" text="Tagliste" translateX="-200.0">
                     <font>
                        <Font size="32.0" />
                     </font>
                  </Button>
                  <Button fx:id="importButton" alignment="TOP_CENTER" mnemonicParsing="false" onAction="#switchToImport" text="Import" translateX="-200.0">
                     <font>
                        <Font size="32.0" />
                     </font>
                  </Button>
              </buttons>
            </ButtonBar>
         </top>
         <bottom>
            
                  <Label text="Secondary View" BorderPane.alignment="CENTER">
               <font>
                  <Font size="32.0" />
               </font>
            </Label>
         </bottom>
         <center>
            <AnchorPane prefHeight="430.0" prefWidth="57.0" BorderPane.alignment="CENTER">
               <children>
                  <ListView fx:id="lvImportedFiles" prefHeight="353.0" prefWidth="1040.0" />
               </children>
            </AnchorPane>
         </center>
      </BorderPane>
      <Button mnemonicParsing="false" text="Button" onAction="#extractNamesFromTagFile" />
       <Button mnemonicParsing="false" text="Export Files" onAction="#exportFiles" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
