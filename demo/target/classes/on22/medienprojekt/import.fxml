<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="500.0" prefWidth="500.0" spacing="20.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="on22.medienprojekt.ImportController">
   <children>
      <ButtonBar buttonMinWidth="110.0" prefHeight="40.0" prefWidth="200.0">
        <buttons>
          <Button id="startButton" fx:id="startButton" alignment="TOP_CENTER" mnemonicParsing="false" onAction="#switchToStart" text="Start" />
            <Button id="managementButton" fx:id="managementButton" alignment="TOP_CENTER" mnemonicParsing="false" onAction="#switchToManagement" text="Verwaltung" />
            <Button id="taglistButton" fx:id="taglistButton" alignment="TOP_CENTER" mnemonicParsing="false" onAction="#switchToTaglist" text="Tagliste" />
            <Button id="importButton" fx:id="importButton" alignment="TOP_CENTER" defaultButton="true" mnemonicParsing="false" onAction="#switchToImport" text="Import" />
        </buttons>
      </ButtonBar>
      <Label text="Import View" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
