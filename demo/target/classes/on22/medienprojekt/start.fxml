<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="720.0" prefWidth="1280.0" spacing="20.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="on22.medienprojekt.StartController">
   <children>
      <ButtonBar buttonMinWidth="110.0" prefHeight="40.0" prefWidth="200.0" translateY="-220.0">
        <buttons>
          <Button fx:id="startButton" alignment="CENTER" defaultButton="true" mnemonicParsing="false" onAction="#switchToStart" text="Start" translateX="-200.0">
               <font>
                  <Font size="32.0" />
               </font></Button>
            <Button fx:id="managementButton" alignment="CENTER" mnemonicParsing="false" onAction="#switchToManagement" text="Verwaltung" translateX="-200.0">
               <font>
                  <Font size="32.0" />
               </font></Button>
            <Button fx:id="taglistButton" alignment="TOP_CENTER" mnemonicParsing="false" onAction="#switchToTaglist" text="Tagliste" translateX="-200.0">
               <font>
                  <Font size="32.0" />
               </font></Button>
            <Button fx:id="importButton" alignment="TOP_CENTER" mnemonicParsing="false" onAction="#switchToImport" text="Import" translateX="-200.0">
               <font>
                  <Font size="32.0" />
               </font></Button>
        </buttons>
      </ButtonBar>
      <Label text="Primary View">
         <font>
            <Font size="32.0" />
         </font></Label>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
